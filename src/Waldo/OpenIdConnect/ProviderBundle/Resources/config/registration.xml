<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    
    <parameters>
        <parameter key="waldo_oic_p.registration.user.class">Waldo\OpenIdConnect\ProviderBundle\Services\RegistrationUserService</parameter>
    </parameters>

    <services>
        
        <service id="waldo_oic_p.registration.user" class="%waldo_oic_p.registration.user.class%">
            <argument type="service" id="doctrine.orm.default_entity_manager" />            
            <argument type="service" id="logger" on-invalid="null" />
        </service>
        
    </services>
    
</container>
