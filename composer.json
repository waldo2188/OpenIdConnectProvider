{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": {
            "": "src/",
            "SymfonyStandard": "app/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.5.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "gree/jose": "~0.2",
        "braincrafted/bootstrap-bundle": "~2.1",
        "twbs/bootstrap": "3.2.*",
        "jquery/jquery": "1.11.*",
        "datatables/datatables": "dev-master",
        "DataTables/Plugins": "dev-master",
        "ali/datatable": "dev-master",
        "almasaeed2010/AdminLTE" : "dev-master",
        "knplabs/knp-menu-bundle": "~2.0",
        "cnerta/breadcrumb-bundle": "~2.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "phpunit/phpunit": "~4.2",
        "scribe/cssembed-library": "~0.4",
        "nervo/yuicompressor": "~2.4"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install",
            "cp -R vendor/datatables/datatables/media/* app/Resources/public/datatable",
            "rm -rf app/Resources/public/datatable/unit_testing"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install",
            "cp -R vendor/datatables/datatables/media/* app/Resources/public/datatable",
            "rm -rf app/Resources/public/datatable/unit_testing"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:wtfred/AliDatatableBundle.git"
        },
        {
            "type": "git",
            "url": "git@github.com:waldo2188/jose.git"
        },
        {
            "type": "git",
            "url": "git@github.com:waldo2188/bootstrap-datetimepicker.git"
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.11.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.11.1.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "almasaeed2010/AdminLTE",
                "version": "master",
                "source": {
                    "url": "https://github.com/almasaeed2010/AdminLTE.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "DataTables/Plugins",
                "version": "master",
                "source": {
                    "url": "https://github.com/DataTables/Plugins.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ]
}
